{
  "name": "johnridesa.bike",
  "version": "1.0.0",
  "private": true,
  "description": "John Jackson's personal site",
  "keywords": [
    "gatsby"
  ],
  "bugs": {
    "url": "https://github.com/johnridesabike/johnridesabike.github.io/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/johnridesabike/johnridesabike.github.io"
  },
  "license": "MIT",
  "author": "John Jackson",
  "scripts": {
    "build": "concurrently yarn:build:*",
    "build:css": "postcss style.css -d _site",
    "build:11ty": "ELEVENTY_EXPERIMENTAL=true eleventy",
    "clean": "rm -r _site",
    "deploy": "yarn build && gh-pages -d public -b master",
    "start": "concurrently yarn:start:*",
    "start:11ty": "ELEVENTY_EXPERIMENTAL=true eleventy --serve",
    "start:css": "postcss style.css -d _site -w"
  },
  "dependencies": {
    "feather-icons": "^4.28.0",
    "normalize.css": "^8.0.1",
    "postcss-custom-properties": "^9.1.1",
    "prism": "^4.1.2",
    "prismjs": "^1.21.0",
    "simple-icons": "^3.2.0",
    "typeface-libre-baskerville": "0.0.72",
    "typeface-source-code-pro": "^1.1.3",
    "typeface-source-sans-pro": "1.1.5"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-img": "^0.3.0",
    "concurrently": "^5.3.0",
    "cssnano": "^4.1.10",
    "fast-glob": "^3.2.4",
    "gh-pages": "^3.1.0",
    "markdown-it-footnote": "^3.0.2",
    "markdown-it-implicit-figures": "^0.10.0",
    "postcss": "^8.1.7",
    "postcss-cli": "^8.3.0",
    "postcss-custom-media": "^7.0.8",
    "postcss-import": "^13.0.0"
  }
}
